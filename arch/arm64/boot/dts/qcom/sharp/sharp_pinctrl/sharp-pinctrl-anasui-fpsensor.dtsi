&tlmm {
	/*****************************************/
	/*  pinctrl customize setting            */
	/*****************************************/

	fpc_reset_reset: fpc_reset_reset {
		mux {
			pins = "gpio12";
			function = "gpio";
		};
		config {
			pins = "gpio12";
			drive-strength = <4>;
			bias-disable;
			output-low;
		};
	};
	fpc_reset_active: fpc_reset_active {
		mux {
			pins = "gpio12";
			function = "gpio";
		};
		config {
			pins = "gpio12";
			drive-strength = <4>;
			bias-disable;
			output-high;
		};
	};

	fpc_irq_active: fpc_irq_active {
		mux {
			pins = "gpio48";
			function = "gpio";
		};
		config {
			pins = "gpio48";
			drive-strength = <2>;
			bias-pull-down;
		};
	};
};
