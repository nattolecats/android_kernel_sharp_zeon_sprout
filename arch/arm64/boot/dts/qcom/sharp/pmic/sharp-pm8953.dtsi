&soc {
	/*---------------------------------------*/
	/*  Battery Control Driver               */
	/*---------------------------------------*/
	sharp,shbatt {
		compatible = "sharp,shbatt";
		sharp,cl-high-thresh = <45>;
		sharp,cl-low-thresh = <45>;
		status = "ok";
	};
};
