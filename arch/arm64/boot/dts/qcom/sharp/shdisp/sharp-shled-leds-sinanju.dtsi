&soc {
	/*-------------------------------*/
	/* LED color variation parameter */
	/*-------------------------------*/
	sharp,shled_leds_color_variation {
		sharp,system-color-default = <0x0e>;

		sharp,shled-leds-calib-0e {
			red   = <15>;
			green = <10>;
			blue  = <30>;
		};

		sharp,shled-leds-calib-04 {
			red   = <15>;
			green = <10>;
			blue  = <30>;
		};

		sharp,shled-leds-calib-02 {
			red   = <15>;
			green = <10>;
			blue  = <30>;
		};
	};
};
