/*****************************************/
/*  pm8953: qcom,pm8953 alias            */
/*****************************************/
&spmi_bus {
	qcom,pm8953@0 {
		qcom,power-on@800 {
			qcom,pon_1 {
			};

			qcom,pon_2 {
				linux,code = <115>;
			};
		};
	};
};
